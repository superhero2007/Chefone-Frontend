{
  "name": "c1-frontend",
  "main": "src/index.js",
  "engines": {
    "node": "v8.2.1"
  },
  "scripts": {
    "format": "npx prettier-eslint-cli --trailing-comma=\"all\" --semi --single-quote --bracket-spacing --write \"styles/**/*.less\" \"src/**/*.less\" \"src/**/*.js\" \"scripts/**/*.js\"",
    "prepush": "node -v && npm run test",
    "install-git-hooks": "cross-env rm -rf .git/hooks && node node_modules/husky/bin/install && cp -r hooks/. .git/hooks/",
    "update-api": "cd ./src/parseApi && bash ./updateSchemas.sh && babel-node ./script",
    "update-api-dev": "cd ./src/parseApi && bash ./updateSchemasDev.sh && babel-node ./script",
    "test": "node_modules/.bin/eslint src --color",
    "lint": "node_modules/.bin/eslint src --color",
    "lint-watch": "node_modules/.bin/esw src --color -w",
    "start": "concurrently --kill-others \"npm run watch-client\" \"npm run run-dev-server\"",
    "start-prod": "NODE_ENV=production node src/server",
    "deploy": "cross-env NODE_ENV=production node scripts/deploy.js",
    "playground": "cross-env playground=true npm run start",
    "build-dev-server": "cross-env NODE_ENV=development TARGET=server webpack --config webpack.config.babel",
    "run-dev-server": "cross-env NODE_ENV=development node ./build/server",
    "watch-client": "cross-env DLL=main better-npm-run watch-client",
    "clean:build": "rimraf dist/build && mkdirp dist/build",
    "build": "npm run clean:build && npm run build:webpack && npm run start-cp",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.babel",
    "prepare-dev": "rm -rf build && concurrently --kill-others \"npm run build:webpack:dev-dll\" \"npm run build-dev-server\"",
    "build:webpack:dev-dll": "cross-env NODE_ENV=development DLL=vendor TARGET=client webpack --progress --config webpack.config.babel",
    "build-deploy": "npm run build && npm run deploy",
    "start-cp:package": "cp ./package.json ./dist/build",
    "start-cp:lock": "cp ./package-lock.json ./dist/build",
    "start-cp:static": "cp -r ./static/copy ./dist/build/static",
    "start-cp:font": "cp -r ./font ./dist/build",
    "start-cp:webpack": "cp -r ./webpack.config.babel ./dist/build",
    "start-cp:styles": "cp -r ./styles ./dist/build",
    "start-cp": "npm run start-cp:webpack && npm run start-cp:styles && npm run start-cp:package && npm run start-cp:lock && npm run start-cp:static && npm run start-cp:font",
    "version": "npm run build && git add public/* ",
    "postversion": "git push && git push --tags",
    "catl": "node_modules/.bin/catl /ci"
  },
  "betterScripts": {
    "watch-client": {
      "command": "node webpack.config.babel/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 128
      }
    }
  },
  "license": "ISC",
